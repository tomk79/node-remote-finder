{
  "name": "remote-finder",
  "version": "0.3.0",
  "private": false,
  "description": "Finder や Explorer のように、サーバー上のファイルとフォルダを操作するUIを提供します。",
  "main": "node/main.js",
  "scripts": {
    "start": "node ./tests/app/server/server.js;",
    "up": "node ./tests/app/server/server.js;",
    "preview": "open http://127.0.0.1:3000/;",
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "test": "mocha --ui tdd ./tests/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomk79/node-remote-finder.git"
  },
  "author": "Tomoya Koyanagi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomk79/node-remote-finder/issues"
  },
  "homepage": "https://github.com/tomk79/node-remote-finder#readme",
  "dependencies": {
    "es6-promise": "^4.1.0",
    "file-type": "^14.1.3",
    "fs-extra": "^7.0.1",
    "iterate79": "^1.0.0",
    "mime-types": "^2.1.26",
    "utils79": "0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "axios": "^0.25",
    "babel-loader": "^8.2.4",
    "body-parser": "^1.17.1",
    "cheerio": "*",
    "csv-loader": "^3.0.3",
    "dateformat": "^5.0.3",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "gitparse79": "github:tomk79/node-git-parser#develop",
    "gitui79": "github:tomk79/gitui79.js#develop",
    "iterate79": "^1.1.1",
    "jquery": "^3.6.0",
    "langbank": "github:tomk79/node-langbank.git#develop",
    "laravel-mix": "^6.0.48",
    "lodash": "^4.17.21",
    "marked": "^4.0.12",
    "mocha": "^3.2.0",
    "postcss": "^8.1.14",
    "px2style": "github:pickles2/px2style#develop",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.9",

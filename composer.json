{
    "name": "tomk79/remote-finder",
    "description": "Finder や Explorer のように、サーバー上のファイルとフォルダを操作するUIを提供します。",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "scripts":{
        "start": [
            "php -S 127.0.0.1:8088 -t ./"
        ],
        "open-in-browser": [
            "open http://127.0.0.1:8088/tests/app/client_php/"
        ],
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ]
    } ,
    "minimum-stability": "dev",
    "repositories": [
    ],
    "require": {
        "php": ">=5.4.0",
        "tomk79/filesystem": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1.0"
    },
    "autoload": {
        "files": [
            "php/main.php"
        ]
    }
}
